    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.3",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.4",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.5.3",
