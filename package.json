{
  "name": "angular-webpack-quickstart",
  "version": "0.0.1",
  "private": true,
  "description": "Angular QuickStart with Webpack 2 - source from the documentation.",
  "scripts": {
    "clean": "rimraf src/main.js src/main.min.js src/*.map",
    "build": "run-s build:prod",
    "build:dev": "webpack --config webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production webpack -p --config webpack.config.js",
    "build:e2e": "tsc -p e2e/",
    "rebuild": "run-s clean build",
    "serve": "lite-server -c=bs-config.json",
    "serve:e2e": "lite-server -c=bs-config.e2e.json",
    "start": "run-p watch serve",
    "start-wp-dev-server": "webpack-dev-server",
    "pree2e": "run-s build:e2e",
    "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js",
    "test": "karma start karma.conf.js",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "watch": "webpack --config webpack.config.js --watch"
  },
  "keywords": [
    "Angular",
    "Angular2",
    "Angular4",
    "Quickstart",
    "Webpack",
    "Webpack2",
    "TypeScript",
    "SCSS",
    "PostCSS",
    "CI",
    "scaffolding",
    "Getting started",
    "Starter"
  ],
  "author": "shellyln",
  "license": "MIT",
  "dependencies": {
    "systemjs": "0.19.40",
    "core-js": "^2.4.1"
  },
  "devDependencies": {
    "@angular/common": "~4.0.0",
    "@angular/compiler": "~4.0.0",
    "@angular/core": "~4.0.0",
    "@angular/forms": "~4.0.0",
    "@angular/http": "~4.0.0",
    "@angular/platform-browser": "~4.0.0",
    "@angular/platform-browser-dynamic": "~4.0.0",
    "@angular/router": "~4.0.0",
    "@types/jasmine": "^2.5.52",
    "@types/node": "^7.0.31",
    "angular-in-memory-web-api": "~0.3.0",
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "canonical-path": "0.0.2",
    "concurrently": "^3.2.0",
    "cpx": "^1.5.0",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.5",
    "jasmine-core": "~2.4.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
